{
    "name": "sorokinmedia/yii2-notificator",
    "description": "Notificator for Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2", "yii 2", "notification", "telegram", "e-mail"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ruslan Gilyazetdinov",
            "email": "ma3obblu@gmail.com",
            "homepage": "https://www.facebook.com/Ma3oBblu"
        }
    ],
    "support": {
        "issues": "https://github.com/sorokinmedia/yii2-notificator/issues?state=open",
        "source": "https://github.com/sorokinmedia/yii2-notificator"
    },
    "require": {
        "yiisoft/yii2": ">=2.0.30",
        "sorokinmedia/yii2-ar-relations": "dev-master",
        "sorokinmedia/yii2-helpers": "dev-master"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "8.*"
    },
    "autoload": {
        "psr-4": {
            "sorokinmedia\\notificator\\": "src/",
            "sorokinmedia\\notificator\\tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
